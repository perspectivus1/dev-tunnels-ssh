{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Tests",
			"program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
			"args": [
				"--require",
				"source-map-support/register",
				"out/lib/ssh-test/*Tests.js"
			],
			"skipFiles": [
				"<node_internals>/**/*.js"
			],
			"env": {
				"DEBUG": "dev-tunnels-ssh"
			},
			"console": "integratedTerminal"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Benchmarks",
			"program": "${workspaceFolder}/out/lib/ssh-bench/main",
			"args": [],
			"skipFiles": [
				"<node_internals>/**/*.js"
			],
			"env": {
				"DEBUG": "dev-tunnels-ssh"
			},
			"console": "integratedTerminal"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Browser Test Server",
			"program": "${workspaceFolder}/out/lib/ssh-test/cli",
			"args": [
				"sshd",
				"-w",
				"-p",
				"9880"
			],
			"skipFiles": [
				"<node_internals>/**/*.js"
			],
			"env": {
				"DEBUG": "dev-tunnels-ssh"
			},
			"console": "integratedTerminal"
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach Debugger",
			"skipFiles": [
				"<node_internals>/**/*.js"
			],
			"stopOnEntry": false
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Node.js server",
			"program": "${workspaceFolder}/out/lib/ssh-test/cli",
			"args": [
				"sshd",
				"-p",
				"9881",
				"-o",
				"AuthorizedKeysFile=/home/i034929/.ssh/authorized_keys",
				"-o",
				"LogLevel=verbose"
			],
			"skipFiles": [
				"<node_internals>/**/*.js"
			],
			"env": {
				"DEBUG": "dev-tunnels-ssh"
			},
			"console": "integratedTerminal",
			"sourceMaps": true
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Node.js client",
			"program": "${workspaceFolder}/out/lib/ssh-test/cli",
			"args": [
				"ssh",
				"-p",
				"9881",
				"localhost",
				"-l",
				"i034929",
				"-o",
				"UserKnownHostsFile=/tmp/known_hosts",
				"-o",
				"IdentityFile=/home/i034929/.ssh/id_rsa.2",
				// "ls",
				"-L",
				"8080:www.ubuntuforums.org:80",
			],
			"skipFiles": [
				"<node_internals>/**/*.js"
			],
			"env": {
				"DEBUG": "dev-tunnels-ssh"
			},
			"console": "integratedTerminal",
			"sourceMaps": true
		},
	]
}